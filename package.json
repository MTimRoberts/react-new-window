{
  "name": "react-new-window",
  "description": "Pop a new window in React, using window.open API",
  "version": "1.0.1",
  "keywords": [
    "react",
    "window",
    "open",
    "popup"
  ],
  "main": "./dist/react-new-window.umd.js",
  "module": "./dist/react-new-window.mjs",
  "exports": {
    ".": {
      "import": "./dist/react-new-window.mjs",
      "require": "./dist/react-new-window.umd.js"
    }
  },
  "repository": "git@github.com:rmariuzzo/react-new-window.git",
  "author": "Rubens Mariuzzo <rubens@mariuzzo.com>",
  "license": "MIT",
  "files": [
    "dist",
    "types"
  ],
  "types": "./types/NewWindow.d.ts",
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-storysource": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.13",
    "@testing-library/react": "^13.4.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^18.0.0"
  },
  "scripts": {
    "build": "vite build",
    "build-storybook": "cross-env NODE_ENV=test NODE_OPTIONS=--openssl-legacy-provider build-storybook -o docs",
    "test": "vitest",
    "postversion": "git push origin && git push origin --tags",
    "storybook": "cross-env NODE_ENV=test NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006"
  }
}
